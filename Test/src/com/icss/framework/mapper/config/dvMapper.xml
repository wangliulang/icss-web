<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dv.mapper">
	<!--  新增记录  -->
    <insert id="insertUser" parameterType="Map" useGeneratedKeys="true" keyProperty="record_id">
         insert into s_mjxt_record_info(mac,qrcode,generationtime,state,ipid) values(#{m_value},#{q_value},#{g_value},#{s_value},#{i_value})
    </insert>
    
	<!--  查询单条记录  
    <select id="selectBlog" parameterType="int" resultMap="BlogResult">
       select * from t_blog where doctype_id = #{id}
    </select>
    -->
	<!--  修改记录  
    <update id="updateBlog" parameterType="Blog">
        update t_blog set title = #{title},content = #{content},owner = #{owner} where id = #{id}
    </update>
    -->
	<!--  查询所有记录  -->
    <select id="dv_selectAll" resultType="Map" parameterType="string">
        <!-- select * from t_mjxt_yz_info where created_lr = #{lr} -->
        select * from t_mjxt_yz_info where created_lr = #{lr}

        
    </select>
	<!--  删除记录  
    <delete id="deleteBlog" parameterType="int">
       delete from t_blog where id = #{id}
    </delete>
    -->
</mapper>